  <Code>Dim line As Boolean = True
Dim lastId as Int32 = 0

Public Function IsRowPair(ByVal id As Int32 ) As Boolean
  If lastId &lt;&gt; id Then
    line = Not line
    lastId = id
  End If
  Return line
End Function

Public Function GetFile(ByRef line As String) As String
  Return System.IO.Path.GetFileName(line)
End Function</Code>
